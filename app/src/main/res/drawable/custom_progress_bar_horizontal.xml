<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners
                    android:radius="5dip" />
                <gradient
                    android:startColor="#ff00e30f"
                    android:endColor="#ffd2462c"
                    android:angle="180" />
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <corners
                    android:radius="5dip" />
                <gradient
                    android:startColor="#ff00e30f"
                    android:endColor="#ffd2462c"
                    android:angle="180" />
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/background">
        <clip>
            <shape>
                <corners
                    android:radius="5dip" />
                <gradient
                    android:startColor="#ff00e30f"
                    android:endColor="#ffd2462c"
                    android:angle="180" />
            </shape>
        </clip>
    </item>
</layer-list>